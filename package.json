    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-server": "nodemon ./server/index.js",
    "start-dev": "mongod & npm run start-server & npm run watch-css & npm start"
